{
  "exclude": [
    "**/node_modules/**",
  ],
  "compilerOptions": {
    "target": "ES2017",
    "experimentalDecorators": true,
    "module": "ESNext",
    "rootDirs": [
      "src",
      "docs",
    ],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipDefaultLibCheck": false,
    "skipLibCheck": true,
    "paths": {
      "vue-memoize-dict": [
        "./src/index.ts"
      ],
    }
  }
}
